public class BankAccountTester
{
	public static void main(String[] args)
	{
		//This line creates a new BankAccount object called harrysChecking with 0 balance
		BankAccount harrysChecking = new BankAccount();

		//This line calls the deposit method for harrysChecking
		harrysChecking.deposit(2000);

		//This line calls the withdrawal method for harrysChecking
		harrysChecking.withdraw(500);

		//This line calls the getBalance method and prints it     
		System.out.println("Harry's Checking Balance: " + harrysChecking.getBalance());
   
		//creates new BankAccount Object called mySavings
		BankAccount mySavings = new BankAccount();
		mySavings.balance = 1000.00;
		
		//calls the withdraw method for mySavings
		mySavings.withdraw(1001.00);
		
		System.out.println("Harry's Savings Account Balance: " + mySavings.getBalance());
		
	}
}
